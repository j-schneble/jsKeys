{
    "description": "left_command + 3 = open Safari",
    "manipulators": [
        {
            "from": {
                "key_code": "3",
                "modifiers": { "mandatory": ["left_command"] }
            },
            "to": [{ "shell_command": "open '/Applications/Safari.app'" }],
            "type": "basic"
        }
    ]
}
